<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
    "http://www.ibatis.com/dtd/sql-map-config-2.dtd">

<sqlMapConfig>	
   <settings 
   	enhancementEnabled="true" 
   	cacheModelsEnabled="true"
   	lazyLoadingEnabled="true"
   	useStatementNamespaces="true"   	
   />
   
   <sqlMap resource="resources/sqlmap/cpt/myp/cptMyInfo/CptMyInfo_SQL.xml"/>            <!-- 기업정보 -->
   <sqlMap resource="resources/sqlmap/cpt/myp/api/CptApiManage_SQL.xml"/>               <!-- API 관리 -->
   <sqlMap resource="resources/sqlmap/cpt/myp/app/CptAppManage_SQL.xml"/>               <!-- APP 관리 -->
   <sqlMap resource="resources/sqlmap/cpt/myp/svcTerms/CptServiceTerms_SQL.xml"/>       <!-- 금융정보 제공동의서 -->
   <sqlMap resource="resources/sqlmap/cpt/myp/stats/CptStatsTraffic_SQL.xml"/>          <!-- 트레픽 통계 -->
</sqlMapConfig>
