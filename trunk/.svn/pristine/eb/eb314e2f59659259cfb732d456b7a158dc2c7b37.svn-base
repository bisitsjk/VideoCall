                                                                <span class="right_area"><em class="ft_black">{{data.maturity | date:'yyyy-MM-dd'}}</em></span>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                    <!--// loop -->
                                                </div>
                                            </div>
                                            <!--// 펀드 -->
                                            <!-- 기타 -->
                                            <div class="cont">
                                                <div class="card_tb">
                                                    <!-- loop -->
                                                    <div class="card_tb_cont" ng-repeat="data in vm.portfolioEtcList">
                                                        <div class="card_head">
															<span class="left_area">
																<span class="biz">{{data.assetName}}</span>
