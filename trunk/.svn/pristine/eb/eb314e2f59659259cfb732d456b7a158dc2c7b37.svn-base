                                            </div>
                                            <!--// 주식 -->
                                            <!-- 펀드 -->
                                            <div class="cont">
                                                <div class="card_tb">
                                                    <!-- loop -->
                                                    <div class="card_tb_cont" ng-repeat="data in vm.portfolioFundList">
                                                        <div class="card_head">
															<span class="left_area">
																<span class="biz">{{data.fundName}}</span>
															</span>
                                                            <span class="right_area">{{data.comName}}</span>
                                                        </div>
                                                        <ul class="card_body">
