/* table */
table     {width:100%;border-collapse:collapse;border-spacing:0;}
tbody, tfoot, thead, tr, th, td {table-layout:fixed;word-break:break-all;}
caption   {display:none;}
.tit {font-size:20px;color:#515151;font-weight:bold;padding:10px 0 5px 5px;}
.table_row {border-top:1px solid #666;border-bottom:2px solid #999;margin-bottom:20px;}
.table_row table {width:100%;}
.table_row table tbody tr.sky th, .table_row table tbody tr.sky td {background:#d2f6f6;color:#000;}
.table_row table tbody tr.yellow td {background:#ffeeca;color:#000;}
.table_row table tbody tr.yellow td:first-child {background:none;}
.table_row table thead th {padding:5px;background:#d9d9d9;border-left:1px solid #c3c3c3;border-bottom:1px solid #666;color:#333;}
.table_row table tbody td {padding:5px;border-top:1px solid #c3c3c3;border-left:1px solid #c3c3c3;}
.table_row table tbody th {text-align:center;background:#f9f9f9;color:#515151;border-top:1px solid #c3c3c3;}
.table_row table tbody tr:first-child th, .table_row table tbody tr:first-child td {border-top:0;}
.table_row table th:first-child, .table_row table tbody td:first-child {border-left:0;}

.bgY {background:#ffffe3;}
.bgO {background:#f47725;}
.bgR {background:#e31836;}
.text_area {padding:5px;margin-bottom:10px;color:#666;border:2px solid #ccc;line-height:1.5;}
.text_area strong {display:inline-block;}
.text_area strong:first-child {padding-left:0;}
.text_area span {display:inline-block;width:10px;height:10px;vertical-align:center;}
tr:hover th, tr:hover td {background:#f5f5f5 !important;}
.red {color:red;}
.blue {color:blue;}
</style>
